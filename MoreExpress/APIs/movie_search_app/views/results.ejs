<h1>Results Page</h1>

<% if(!!data && data["Response"] !== "False") { %>
    <% console.log(data); %>
    <ul>
        <% data["Search"].forEach(function(movie){ %>
            <li>
                <strong>
                    <%= movie["Title"] %>
                </strong>
                - <%= movie["Year"] %>
            </li>
        <% }); %>
    </ul>
<% } %>

<a href="/">Search Again!</a>